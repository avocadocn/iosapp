ion-view
  ion-header-bar
    a.button.icon.ion-chevron-left(ng-href="#/team/{{team._id}}") &nbsp;返回
    h1.title 队员
  ion-content
    .team_box
      .team_name
        h2(ng-cloak) {{team.name}}
      .team_member(ng-repeat="member in members")
        img.logo(ng-src="{{STATIC_URL + member.photo}}/45/45")
        p.name {{member.nickname}}


