ion-view
  ion-header-bar
    a.button.icon.ion-chevron-left(ng-href="{{backUrl}}") &nbsp;返回
    h1.title 队员(成员)
  ion-content
    div(ng-repeat="memberContent in memberContents")
      p {{memberContent.name}}
      .row
        .member.col-25(ng-repeat="member in memberContent.members")
          img(ng-src="{{STATIC_URL + member.photo}}/45/45")
          p {{member.nickname}}
