ion-view
  ion-header-bar
    a.button.icon-left.ion-chevron-left(ng-href="#/company/team_page") &nbsp;返回
    h1.title {{title}}
  ion-content
    ul.list
      a.item.item-avatar(ng-repeat="team in teams")
        img(ng-src="{{STATIC_URL + team.logo + '/45/45'}}")
        h2 {{team.name}}
        p 类型: {{team.groupType}}
        //- p 队长: todo
        p 打开: {{team.active}}
        p 成员: {{team.memberCount}}
        p 积分: {{team.score.total}}